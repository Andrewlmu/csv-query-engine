# OpenAI API Key (required)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Reducto Configuration (Optional - Enhanced PDF Parsing)
# Get API key from: https://studio.reducto.ai/
USE_REDUCTO=false
REDUCTO_API_KEY=your-reducto-api-key-here

# Agentic RAG Configuration (Optional - Autonomous Agent)
# Enable ReAct pattern agent with tool use and multi-step reasoning
USE_AGENTIC_RAG=false
AGENT_MAX_LOOPS=10
AGENT_TIMEOUT=60000
AGENT_VECTOR_SEARCH_MAX=5
AGENT_MEMORY_ENABLED=false
AGENT_MEMORY_TYPE=in-memory
AGENT_LOGGING_ENABLED=true
AGENT_TRACE_STEPS=false

# Optional: For Railway deployment
# RAILWAY_ENVIRONMENT=production

# Optional: Custom settings
# MAX_CHUNKS_PER_QUERY=5
# EMBEDDING_MODEL=text-embedding-ada-002
# LLM_MODEL=gpt-5